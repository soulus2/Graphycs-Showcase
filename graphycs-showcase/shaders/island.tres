code = "shader_type canvas_item;
render_mode blend_mix;

uniform sampler2D noise : filter_nearest;
uniform float water_level : hint_range(0.0, 1.0);
uniform float Shore_line_width;
uniform vec4 shore_line : source_color;
uniform float water_depth;
uniform vec4 water : source_color;
uniform vec4 land : source_color;



void fragment() {
	vec4 n_out3p0;
// Texture2D:3
	n_out3p0 = texture(noise, UV);


// FloatParameter:10
	float n_out10p0 = water_level;


// FloatParameter:14
	float n_out14p0 = Shore_line_width;


// ColorParameter:5
	vec4 n_out5p0 = shore_line;


// ColorOp:15
	vec3 n_out15p0 = (vec3(n_out3p0.xyz)) / (vec3(1.0) - vec3(n_out5p0.xyz));


// FloatOp:11
	float n_in11p0 = 1.00000;
	float n_out11p0 = n_in11p0 - n_out3p0.x;


// FloatParameter:16
	float n_out16p0 = water_depth;


// FloatOp:13
	float n_out13p0 = pow(n_out11p0, n_out16p0);


// FloatOp:12
	float n_out12p0 = n_out10p0 + n_out13p0;


// ColorParameter:6
	vec4 n_out6p0 = water;


// ColorOp:9
	vec3 n_out9p0 = vec3(1.0) - (vec3(1.0) - vec3(n_out12p0)) / (vec3(n_out6p0.xyz));


// ColorParameter:7
	vec4 n_out7p0 = land;


// ColorOp:8
	vec3 n_out8p0 = vec3(1.0) - (vec3(1.0) - vec3(n_out3p0.xyz)) * (vec3(1.0) - vec3(n_out7p0.xyz));


	vec3 n_out4p0;
// If:4
	if(abs(n_out3p0.x - n_out10p0) < n_out14p0)
	{
		n_out4p0 = n_out15p0;
	}
	else if(n_out3p0.x < n_out10p0)
	{
		n_out4p0 = n_out8p0;
	}
	else
	{
		n_out4p0 = n_out9p0;
	}


// Output:0
	COLOR.rgb = n_out4p0;


}
